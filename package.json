{
    "name": "phenixgames-v3",
    "version": "0.0.1",
    "description": "Another PhenixGames Server",
    "private": true,
    "scripts": {
        "init": "python3 _scripts/init.py",
        "dockerStart": "./ragemp-server",
        "start": "docker compose up -d && npm run vueStart && docker logs -f rage",
        "stop": "docker compose down && npm run vueStop",
        "restart": "npm run stop && npm run start",
        "vueStart": "cd phenixgames-v3-vue && docker compose up -d",
        "vueStop": "cd phenixgames-v3-vue && docker compose down",
        "vueRestart": "npm run vueStop && npm run vueStart",
        "format": "docker run --rm -v %cd%:/app -w /app node:18.8 sh -c \"npm install -g prettier && prettier --write .\"",
        "check": "docker run --rm -v %cd%:/app -w /app node:18.8 sh -c \"npm install -g prettier && prettier --check .\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PhenixGames/phenixgames-v3.git"
    },
    "author": "PhenixGames",
    "license": "GLP-3.0",
    "bugs": {
        "url": "https://github.com/PhenixGames/phenixgames-v3/issues"
    },
    "moduleDirectories": [
        "node_modules"
    ],
    "homepage": "https://phenixgames.de",
    "dependencies": {
        "axios": "^1.3.4",
        "bcryptjs": "^2.4.3",
        "caller-id": "^0.1.0",
        "dotenv": "^16.0.3",
        "extends-classes": "^1.0.5",
        "moment": "^2.29.4",
        "mysql2": "^3.2.0",
        "sequelize": "^6.29.3",
        "simple-node-logger": "^21.8.12",
        "validator": "^13.9.0",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "prettier": "^2.8.4",
        "sloc": "^0.2.1"
    }
}
