{
    "name": "phenixgames-v3",
    "version": "0.0.1",
    "description": "Another PhenixGames Server",
    "private": true,
    "scripts": {
        "dockerStart": "./ragemp-server",
        "start": "npm run rm_image && docker compose up -d --build && docker logs -f rage",
        "stop": "docker compose down",
        "restart": "npm run stop && npm run rm_image && npm run start",
        "rm_image": "docker image rm -f phenixgames-v3_vue && docker image rm -f phenixgames-v3_rage && docker image rm -f phenixgames-v3-rage && docker image rm -f phenixgames-v3-vue",
        "format": "prettier --write . --bracket-same-line=true",
        "check": "prettier --check . --bracket-same-line=true"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PhenixGames/phenixgames-v3.git"
    },
    "author": "PhenixGames",
    "license": "GLP-3.0",
    "bugs": {
        "url": "https://github.com/PhenixGames/phenixgames-v3/issues"
    },
    "moduleDirectories": [
        "node_modules"
    ],
    "homepage": "https://phenixgames.de",
    "dependencies": {
        "axios": "^1.2.3",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.0.3",
        "extends-classes": "^1.0.5",
        "moment": "^2.29.4",
        "mysql2": "^3.0.1",
        "sequelize": "^6.28.0",
        "simple-node-logger": "^21.8.12",
        "validator": "^13.7.0",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "prettier": "^2.8.4",
        "sloc": "^0.2.1"
    }
}
